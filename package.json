{
  "name": "knex-rqlite",
  "amdName": "knex-rqlite",
  "version": "2.0.3",
  "description": "Rqlite dialect for knex",
  "source": "lib/main.ts",
  "main": "dist/main.js",
  "module": "dist/main.m.js",
  "umd:main": "dist/main.umd.js",
  "esmodule": "dist/main.modern.js",
  "exports": {
    ".": {
      "browser": "./dist/main.m.js",
      "umd": "./dist/main.umd.js",
      "import": "./dist/main.m.js",
      "require": "./dist/main.js"
    },
    "./package.json": "./package.json",
    "./": "./"
  },
  "types": "dist/main.d.ts",
  "authors": [
    "Katja Lutz <opensource@katjalutz.ch>"
  ],
  "homepage": "https://github.com/rqlite/knex-rqlite",
  "repository": {
    "type": "git",
    "url": "https://github.com/rqlite/knex-rqlite.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.3.11",
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "badgen": "^3.2.3",
    "chai": "^5.0.0",
    "fs-extra": "^11.2.0",
    "jest": "^29.7.0",
    "knex": "^3.1.0",
    "microbundle": "^0.15.1",
    "node-notifier": "^10.0.1",
    "prettier": "^3.1.1",
    "rqlite-js": "^2.5.6",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "knex": ">= 3.0.0 < 4",
    "rqlite-js": ">= 2.0.0 < 3"
  },
  "files": [
    "lib",
    "dist"
  ],
  "standard-version": {
    "scripts": {
      "postchangelog": "node scripts/badges.js && git add assets site README.md"
    }
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!(chai)/)"
    ],
    "preset": "ts-jest/presets/js-with-ts",
    "coverageReporters": [
      "json",
      "lcov",
      "text",
      "clover",
      "json-summary"
    ],
    "coveragePathIgnorePatterns": [
      "main.(?:tsx|ts|jsx|js)",
      "/lib/tpl-lib/",
      "/node_modules/"
    ],
    "collectCoverageFrom": [
      "lib/**/*.{ts,tsx,js,jsx}"
    ],
    "testEnvironment": "node"
  }
}
